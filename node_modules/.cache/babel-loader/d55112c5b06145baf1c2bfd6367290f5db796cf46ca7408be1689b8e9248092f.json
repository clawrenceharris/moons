{"ast":null,"code":"import{useEffect,useState}from\"react\";export function useLocalStorage(key,initialValue){const[value,setValue]=useState(()=>{const jsonValue=localStorage.getItem(key);if(jsonValue!=null){return JSON.parse(jsonValue);}else{return initialValue;}});useEffect(()=>{localStorage.setItem(key,JSON.stringify(value));},[key,value]);return[value,setValue];}","map":{"version":3,"names":["useEffect","useState","useLocalStorage","key","initialValue","value","setValue","jsonValue","localStorage","getItem","JSON","parse","setItem","stringify"],"sources":["/Users/caleb/moons/src/hooks/useLocalStorage.js"],"sourcesContent":["import { useEffect, useState } from \"react\";\n\nexport function useLocalStorage(key, initialValue) {\n    const [value, setValue] = useState(() => {\n        const jsonValue = localStorage.getItem(key)\n        if (jsonValue != null) {\n            return JSON.parse(jsonValue);\n        }\n        else {\n            return initialValue;\n        }\n\n    })\n\n    useEffect(() => {\n        localStorage.setItem(key, JSON.stringify(value));\n\n\n    }, [key, value])\n    return [value, setValue]\n}"],"mappings":"AAAA,OAASA,SAAS,CAAEC,QAAQ,KAAQ,OAAO,CAE3C,MAAO,SAAS,CAAAC,eAAeA,CAACC,GAAG,CAAEC,YAAY,CAAE,CAC/C,KAAM,CAACC,KAAK,CAAEC,QAAQ,CAAC,CAAGL,QAAQ,CAAC,IAAM,CACrC,KAAM,CAAAM,SAAS,CAAGC,YAAY,CAACC,OAAO,CAACN,GAAG,CAAC,CAC3C,GAAII,SAAS,EAAI,IAAI,CAAE,CACnB,MAAO,CAAAG,IAAI,CAACC,KAAK,CAACJ,SAAS,CAAC,CAChC,CAAC,IACI,CACD,MAAO,CAAAH,YAAY,CACvB,CAEJ,CAAC,CAAC,CAEFJ,SAAS,CAAC,IAAM,CACZQ,YAAY,CAACI,OAAO,CAACT,GAAG,CAAEO,IAAI,CAACG,SAAS,CAACR,KAAK,CAAC,CAAC,CAGpD,CAAC,CAAE,CAACF,GAAG,CAAEE,KAAK,CAAC,CAAC,CAChB,MAAO,CAACA,KAAK,CAAEC,QAAQ,CAAC,CAC5B"},"metadata":{},"sourceType":"module","externalDependencies":[]}